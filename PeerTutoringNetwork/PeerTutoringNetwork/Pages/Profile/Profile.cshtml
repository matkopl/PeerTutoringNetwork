@page
@model PeerTutoringNetwork.DTOs.ProfileDTO
@{
    ViewData["Title"] = "Profile";
}

<<h2>Your Profile</h2>

<form method="post">
    <div>
        <label>Full Name:</label>
        <input type="text" asp-for="Profile.FullName" />
    </div>
    <div>
        <label>Email:</label>
        <input type="email" asp-for="Profile.Email" />
    </div>
    <div>
        <label>Phone Number:</label>
        <input type="text" asp-for="Profile.PhoneNumber" />
    </div>
    <button type="submit">Update</button>
</form>


